{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dart2js all dev",
            "type": "shell",
            "command": "dart ",
            "args": [
                "--enable-asserts",
                "bin/build.dart"
            ],
            "problemMatcher": [],
            "group": "build"
        },
        {
            "label": "build release",
            "type": "shell",
            "command": "dart",
            "args": [
                "--enable-asserts",
                "bin/build.dart",
                "release"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}
