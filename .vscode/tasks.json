{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "dart2js dev",
            "type": "shell",
            "command": "dart2js",
            "args": [
                "-o",
                "web/main.dart.js",
                "web/main.dart"
            ],
            "problemMatcher": [],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "dart2js release",
            "type": "shell",
            "command": "dart2js",
            "args": [
                "-O4",
                "-o",
                "web/main.dart.js",
                "web/main.dart"
            ],
            "problemMatcher": [],
            "group": "build"
        },
        {
            "label": "dart2native release",
            "type": "shell",
            "command": "dart2native",
            "args": [
                "bin/main.dart",
                "-k",
                "exe",
                "-o",
                "server.exe"
            ],
            "problemMatcher": [],
            "group": "build"
        },
    ]
}
